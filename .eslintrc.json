{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "linebreak-style": "off",
        "quotes": "off",
        "no-lonely-if": "off",
        "max-len": ["off", { "code": 0 }],
        "no-console": "off"
    }
}
